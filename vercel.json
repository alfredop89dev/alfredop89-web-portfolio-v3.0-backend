{
    "version": 2,
    "builds": [
      {
        "src": "/api/send.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/send",
        "methods": ["POST"],
        "dest": "/api/send.js"
      }
    ],
    "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://alfredop89-web-portfolio-v3-0.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
  }