{
  "version": 2,
  "builds": [
    {
      "src": "/api/send.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
       "src": "/api/send(.*)",
       "dest": "/api/send.js",
        "methods": ["POST", "OPTIONS"],
        "headers": {
           "Access-Control-Allow-Origin": "https://alfredop89-web-portfolio-v3-0.vercel.app",
           "Access-Control-Allow-Headers": "Content-Type"
       }
   }
 ]
}